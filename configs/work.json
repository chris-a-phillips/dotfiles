{
  "name": "Work",
  "description": "Comprehensive setup for work development with essential tools",
  "brewfile": "Brewfile-work",
  "macos_defaults_file": "configs/macos_defaults.json",
  "features": {
    "core_tools": true,
    "development": true,
    "productivity": true,
    "cloud_tools": false,
    "extensive_fonts": false,
    "remote_development": false,
    "unity_development": false,
    "gaming": false
  },
  "packages": {
    "additional_brews": [
      "act",
      "asdf",
      "aws-sam-cli",
      "awscli",
      "colordiff",
      "credstash",
      "cryptography",
      "direnv",
      "docutils",
      "git-lfs",
      "gnupg",
      "harlequin",
      "jq",
      "lazysql",
      "mysql",
      "openssl@3",
      "pycparser",
      "qemu",
      "telnet",
      "tlrc",
      "urlview",
      "yq"
    ],
    "additional_casks": [
      "alacritty",
      "db-browser-for-sqlite",
      "dockdoor"
    ]
  },
  "macos_defaults": {
    "enable": true
  },
  "git_config": {
    "enable": true,
    "prompt_for_credentials": true
  },
  "ssh_setup": {
    "enable": false,
    "key_type": "ed25519",
    "key_name": "work"
  },
  "additional_tools": {
    "lazygit": true,
    "powerlevel10k": true,
    "tmux_plugins": true,
    "neovim_lazyvim": true
  },
  "cloud_tools": {
    "aws_cli": true,
    "docker": false,
    "kubernetes": false,
    "terraform": false
  }
} 